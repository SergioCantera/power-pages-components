{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@sections/*": ["src/components/sections//*"],
      "@data/*": ["src/data/*"],
      "@assets/*": ["src/assets/*"],
      "@css/*": ["src/css/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@ts/*": ["src/ts/*"],
      "@/*": ["src/*"],
    }
  },
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
